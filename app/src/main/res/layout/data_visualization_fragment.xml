<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
                                                   xmlns:app="http://schemas.android.com/apk/res-auto"
                                                   xmlns:tools="http://schemas.android.com/tools"
                                                   android:id="@+id/datavisualization"
                                                   android:layout_width="match_parent"
                                                   android:layout_height="match_parent"
                                                   tools:context=".ui.datavisualization.DataVisualization"
                                                   android:background="@color/graycolor">

    <TextView
            android:text="in a year"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView22" android:fontFamily="@font/font_nexa"
            android:textSize="24sp" android:elevation="3dp"
            app:layout_constraintStart_toEndOf="@+id/year_savings"
            android:layout_marginStart="4dp" app:layout_constraintBaseline_toBaselineOf="@+id/year_savings"/>
    <View
            android:layout_width="0dp"
            android:layout_height="350dp"
            android:id="@+id/view3" android:background="@color/colorAccent" android:elevation="1dp"
            app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            app:layout_constraintHorizontal_bias="0.6" android:layout_marginTop="32dp"
            app:layout_constraintTop_toTopOf="parent" android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintVertical_bias="0.0"/>
    <TextView
            android:text="$"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView21"
            android:elevation="3dp" android:fontFamily="@font/font_nexa" android:textSize="24sp"
            app:layout_constraintEnd_toStartOf="@+id/year_savings"
            app:layout_constraintTop_toTopOf="@+id/textView22" android:layout_marginEnd="4dp"/>
    <TextView
            android:text="2091.45"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/year_savings" android:elevation="3dp"
            android:fontFamily="@font/font_nexa" android:textSize="36sp"
            app:layout_constraintStart_toStartOf="@+id/view3" app:layout_constraintEnd_toEndOf="@+id/view3"
            app:layout_constraintHorizontal_bias="0.29"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/view3" android:layout_marginTop="24dp"
            app:layout_constraintTop_toBottomOf="@+id/month_savings"/>
    <TextView
            android:text="423.67"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/month_savings" android:elevation="3dp"
            android:fontFamily="@font/font_nexa" android:textSize="52sp"
            android:layout_marginTop="16dp"
            app:layout_constraintTop_toBottomOf="@+id/day_savings"
            android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="@+id/view3" app:layout_constraintEnd_toEndOf="@+id/view3"/>
    <TextView
            android:text="monthly"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView10" android:fontFamily="@font/font_nexa"
            android:textSize="24sp" android:elevation="3dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/month_savings"
            app:layout_constraintStart_toEndOf="@+id/month_savings" android:layout_marginStart="8dp"/>
    <TextView
            android:text="if you drove the (car that we are doing)"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/subtext_car" android:elevation="3dp"
            android:textSize="12sp" android:fontFamily="@font/font_nexa"
            app:layout_constraintTop_toBottomOf="@+id/textView4"
            app:layout_constraintEnd_toEndOf="@+id/textView4" app:layout_constraintStart_toStartOf="@+id/textView4"/>
    <TextView
            android:text="You would have saved"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:id="@+id/textView4" android:elevation="3dp"
            android:textSize="36sp" android:fontFamily="@font/font_nexa"
            app:layout_constraintTop_toTopOf="@+id/view3"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" app:layout_constraintEnd_toEndOf="@+id/view3"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginTop="24dp"
    />
    <TextView
            android:text="5.73"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/day_savings" android:elevation="3dp"
            android:fontFamily="@font/font_nexa" android:textSize="72sp"
            app:layout_constraintStart_toStartOf="@+id/view3" app:layout_constraintEnd_toEndOf="@+id/view3"
            app:layout_constraintHorizontal_bias="0.32"
            android:layout_marginTop="36dp" app:layout_constraintTop_toTopOf="@+id/subtext_car"/>
    <TextView
            android:text="$"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView7"
            android:elevation="3dp" android:fontFamily="@font/font_nexa" android:textSize="24sp"
            app:layout_constraintEnd_toStartOf="@+id/day_savings" android:layout_marginEnd="12dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/day_savings"/>
    <TextView
            android:text="$"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView12"
            android:elevation="3dp" android:fontFamily="@font/font_nexa" android:textSize="24sp"
            app:layout_constraintTop_toTopOf="@+id/textView10"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toStartOf="@+id/month_savings"/>
    <TextView
            android:text="today"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView8" android:fontFamily="@font/font_nexa"
            android:textSize="24sp" android:elevation="3dp" app:layout_constraintStart_toEndOf="@+id/day_savings"
            android:layout_marginStart="12dp" app:layout_constraintBaseline_toBaselineOf="@+id/day_savings"/>
    <View
            android:layout_width="0dp"
            android:layout_height="163dp"
            android:id="@+id/drivetoday" android:background="@color/colorAccent" android:elevation="2dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toBottomOf="@+id/view3"
            app:layout_constraintStart_toStartOf="@+id/view3" app:layout_constraintEnd_toEndOf="@+id/view3"
            app:layout_constraintHorizontal_bias="1.0"/>
    <TextView
            android:text="today"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView16" android:fontFamily="@font/font_nexa"
            android:textSize="24sp" android:elevation="3dp"
            app:layout_constraintStart_toEndOf="@+id/textView15"
            android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/textView15"/>
    <TextView
            android:text="You drove"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView13" android:fontFamily="@font/font_nexa"
            android:textSize="36sp" android:elevation="3dp" android:layout_marginTop="16dp"
            app:layout_constraintTop_toTopOf="@+id/drivetoday"
            app:layout_constraintStart_toStartOf="@+id/drivetoday" android:layout_marginEnd="16dp"
            app:layout_constraintEnd_toEndOf="@+id/drivetoday" android:layout_marginStart="16dp"/>
    <TextView
            android:text="65"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/mileage" android:elevation="3dp"
            android:fontFamily="@font/font_nexa" android:textSize="72sp"
            app:layout_constraintBottom_toBottomOf="@+id/drivetoday"
            app:layout_constraintTop_toTopOf="@+id/drivetoday" app:layout_constraintVertical_bias="1.0"
            app:layout_constraintStart_toStartOf="@+id/drivetoday" android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="@+id/drivetoday" app:layout_constraintHorizontal_bias="0.323"/>
    <TextView
            android:text="miles"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textView15" android:elevation="3dp"
            android:fontFamily="@font/font_nexa" android:textSize="24sp"
            app:layout_constraintStart_toEndOf="@+id/mileage" android:layout_marginStart="8dp"
            app:layout_constraintBaseline_toBaselineOf="@+id/mileage"/>
    <SeekBar
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:id="@+id/distance_slider" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            style="@android:style/Widget.Material.Light.SeekBar" android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/drivetoday"/>

</androidx.constraintlayout.widget.ConstraintLayout>
